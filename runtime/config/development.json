{
  "api": {
    "port": 8360,
    "workers": 1,
    "stickyCluster": false,
    "startServerTimeout": 3000,
    "reloadSignal": "SIGUSR2",
    "processKillTimeout": 10000,
    "jsonpCallbackField": "callback",
    "jsonContentType": "application/json",
    "jsonpContentType": "application/javascript",
    "errnoField": "errno",
    "errmsgField": "errmsg",
    "defaultErrno": 1000,
    "validateDefaultErrno": 1001,
    "cache": {
      "type": "file",
      "file": {
        "timeout": 86400000,
        "cachePath": "D:\\typescript\\gitdemo\\ebshopServer\\runtime\\cache",
        "pathDepth": 1,
        "gcInterval": 86400000
      }
    },
    "model": {
      "type": "mysql",
      "mysql": {
        "logConnect": true,
        "logSql": true,
        "database": "Shop",
        "prefix": "shop_",
        "encoding": "utf8",
        "host": "127.0.0.1",
        "port": "",
        "user": "root",
        "password": "root",
        "dateStrings": true,
        "acquireWaitTimeout": 3000
      }
    },
    "session": {
      "type": "file",
      "file": {
        "cookie": {
          "name": "thinkjs"
        },
        "sessionPath": "D:\\typescript\\gitdemo\\ebshopServer\\runtime\\session"
      }
    },
    "view": {
      "type": "nunjucks",
      "nunjucks": {
        "viewPath": "D:\\typescript\\gitdemo\\ebshopServer\\view",
        "sep": "_",
        "extname": ".html"
      }
    }
  },
